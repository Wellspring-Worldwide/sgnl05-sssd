# Managed by Puppet.

<% @config.map do |k,v| -%>
<%   if v.is_a?(Hash) -%>
[<%=  k %>]
<%     v.map do |ki, vi| -%>
<%       if ki == 'ldap_user_search_base' || ki == 'ldap_group_search_base' || ki == 'ldap_sudo_search_base' -%
<%=        ki %> = <%= vi.join('?') %>
<%       elsif vi.is_a?(Array) -%>
<%=        ki %> = <%= vi.join(', ') %>
<%       elsif vi != :undef -%>
<%=        ki %> = <%= vi %>
<%       end -%>
<%     end -%>

<%   else -%>
[<%=  k %>]

<%   end -%>
<% end -%>
